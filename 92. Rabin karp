#include<bits/stdc++.h>
vector<int> stringMatch(string &txt, string &pat) {
    vector<int> ans;
            
            int n = txt.size(),m=pat.size();
            
            for(int i=0;i<n-m+1;i++){
                if(pat==txt.substr(i,m)) ans.push_back(i);
            }
            
           
            
            return ans;
}
